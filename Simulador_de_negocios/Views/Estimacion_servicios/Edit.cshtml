@model Simulador_de_negocios.Models.Estimacion_servicios

@{
    ViewData["Title"] = "Edit";
}

<h2>Editar</h2>

<h4>Estimacion_servicios</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="id_suscripcion" />
            <div class="form-group">
                <label asp-for="unidad_produccion" class="control-label"></label>
                <input asp-for="unidad_produccion" class="form-control" />
                <span asp-validation-for="unidad_produccion" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="total_suscripciones" class="control-label"></label>
                <input asp-for="total_suscripciones" class="form-control" />
                <span asp-validation-for="total_suscripciones" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="total_mensual" class="control-label"></label>
                <input asp-for="total_mensual" class="form-control" />
                <span asp-validation-for="total_mensual" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="producto_servicios_id_producto" class="control-label"></label>
                <input asp-for="producto_servicios_id_producto" class="form-control" />
                <span asp-validation-for="producto_servicios_id_producto" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Regresar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
